# /etc/nginx/sites-available/<%= name %>
#
# file managed by Puppet
# do not make any manual change here as it will be overwritten
#
# module nginx

server {
    listen <%= define_port %>;
    server_name <%= server_name %><% server_aliases.each do |val| -%> <%= val %><% end -%>;
    rewrite <%= rewrite_pattern %>;
}

