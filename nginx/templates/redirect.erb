# /etc/nginx/sites-available/<%= name %>
#
# file managed by Puppet
# do not make any manual change here as it will be overwritten
#
# module nginx

server {
    listen <%= port %>;
    server_name <%= define_servername %><% server_aliases.each do |val| -%> <%= val %><% end -%>;
    rewrite ^(.*)$ <%= redirect_url %>$1 <%= define_permanent %>;
}

